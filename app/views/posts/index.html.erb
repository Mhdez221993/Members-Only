<h1>index</h1>
<ul>
  <% @post.each do |post| %>
    <li>
    <% if user_signed_in? %>
        <% @user.each do |user| %>
          <%= user.email if user.id == post.user_id %>
        <% end %>
    <% end %>
      <%= post.title %>
      <%= post.body %>
    </li>
  <% end %>
</ul>

<%= link_to 'Logout', destroy_user_session_path, :method => :delete %>


